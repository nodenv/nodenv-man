{
  "name": "@nodenv/nodenv-man",
  "version": "1.1.0",
  "description": "A nodenv plugin to view man pages for the currently active Node.",
  "homepage": "https://github.com/nodenv/nodenv-man#readme",
  "license": "MIT",
  "contributors": [
    "Jason Karns <jason.karns@gmail.com> (http://jasonkarns.com)"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodenv/nodenv-man.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-man/issues"
  },
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "files": [
    "bin"
  ],
  "scripts": {
    "start": "bin/nodenv-man",
    "test": "bats ${CI:+--tap} test",
    "posttest": "npm run lint",
    "lint": "git ls-files bin etc script */*.*sh | xargs shellcheck",
    "postversion": "git push --follow-tags"
  },
  "devDependencies": {
    "bats": "^1.13.0",
    "bats-assert": "^2.2.4",
    "bats-mock": "^1.2.5",
    "bats-support": "^0.3.0",
    "shellcheck": "^4.1.0"
  }
}
