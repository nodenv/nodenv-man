{
  "name": "nodenv-man",
  "version": "1.1.0",
  "private": true,
  "homepage": "https://github.com/nodenv/nodenv-man#readme",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nodenv/nodenv-man.git"
  },
  "bugs": {
    "url": "https://github.com/nodenv/nodenv-man/issues"
  },
  "directories": {
    "bin": "bin",
    "test": "test"
  },
  "scripts": {
    "test": "bats ${CI:+--tap} test",
    "publish:brew": "brew-publish $npm_package_name v$npm_package_version -r -t nodenv/homebrew-nodenv",
    "postversion": "git push origin master v$npm_package_version && npm run publish:brew"
  },
  "devDependencies": {
    "bats": "^0.4.2",
    "bats-assert": "^1.0.1",
    "bats-mock": "^1.0.1",
    "brew-publish": "git+https://github.com/jasonkarns/brew-publish.git#v1.1.0"
  }
}
